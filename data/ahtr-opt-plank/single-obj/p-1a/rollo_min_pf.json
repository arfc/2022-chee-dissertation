{
    "control_variables": {
        "total_pf": {"min": 0.02, "max": 0.04},
        "sine_a": {"min": 0.0, "max": 2.0},
        "sine_b": {"min": 0.0, "max": 1.57},
        "sine_c": {"min": 0.0, "max": 6.28}
    },
    "evaluators": {
        "openmc_gc": {
            "order": 0,
            "input_script": ["aprun -n 1 -d 1 -- bwpy-environ -- python", "fhr_plank_openmc.py"],
            "execute2": [["aprun -n 19 -d 32 openmc -s 32", ""]], 
            "inputs": ["total_pf", "sine_a", "sine_b", "sine_c"],
            "output_script": ["aprun -n 1 -d 1 -- bwpy-environ -- python", "fhr_plank_openmc_keff.py"],
            "outputs": ["total_pf","keff2"]
        }
    },
    "constraints": {"keff2": {"operator": [">="], "constrained_val": [1.39]}},
    "algorithm": {
        "objective": ["min"],
        "optimized_variable": ["total_pf"],
        "pop_size": 60,
        "generations": 10,
        "parallel": "none"
    }
}
