@article{hassanat_choosing_2019,
	title = {Choosing {Mutation} and {Crossover} {Ratios} for {Genetic} {Algorithms}—{A} {Review} with a {New} {Dynamic} {Approach}},
	volume = {10},
	issn = {2078-2489},
